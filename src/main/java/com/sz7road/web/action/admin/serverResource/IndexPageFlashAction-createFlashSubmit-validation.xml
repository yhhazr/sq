<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.2//EN" "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
<validators>
	<field name="flashFile">
		<field-validator type="required">
			<message key="admin.homepage.validate.image.null.error"></message>
		</field-validator>
	</field>
	
	<field name="flashItem.url">
		<field-validator type="requiredstring">
			<message key="admin.serverResource.validate.url.null.error"></message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">150</param>
			<message key="admin.serverResource.validate.url.length.error"></message>
		</field-validator>
	</field>
</validators>